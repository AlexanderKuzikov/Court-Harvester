# DaData API credentials
# Получить можно на: https://dadata.ru/profile/#info
DADATA_API_KEY=your_api_key_here
DADATA_SECRET_KEY=your_secret_key_here

# Rate limiting settings (optional)
# Максимальное количество параллельных запросов
RATE_LIMIT_MAX_CONCURRENT=5

# Минимальное время между запросами (мс), 50ms = 20 req/sec
RATE_LIMIT_MIN_TIME=50

# Output settings
# Директория для сохранения JSON-файлов
OUTPUT_DIR=./data
